{
    "name": "cloudfox/getnet",
    "description": "Feature pack for internal systems",
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/console": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0",
        "laravel/helpers": "^1.2",
        "nesbot/carbon": "^2.4",
        "nyholm/psr7": "^1.3",
        "vinkla/hashids": "8.*"
    },
    "autoload": {
        "psr-4": {
            "CloudFox\\GetNet\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rupert Lustosa",
            "email": "rupertlustosa@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "CloudFox\\GetNet\\GetNetServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
